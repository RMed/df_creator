      <!-- column-name Categories -->
      <column type="gchararray"/>
    </columns>
    <data>
      <row>
        <col id="0" translatable="yes" comments="Applications related to audio manipulation">Audio</col>
      </row>
      <row>
        <col id="0" translatable="yes" comments="Applications related to video manipulation">Video</col>
      </row>
      <row>
        <col id="0" translatable="yes" comments="Applications related to programming">Development</col>
      </row>
      <row>
        <col id="0" translatable="yes" comments="Applications related to education">Education</col>
      </row>
      <row>
        <col id="0" translatable="yes" comments="Gaming applications">Game</col>
      </row>
      <row>
        <col id="0" translatable="yes" comments="Applications related to graphics manipulation">Graphics</col>
      </row>
      <row>
        <col id="0" translatable="yes" comments="Network related applications">Network</col>
      </row>
      <row>
        <col id="0" translatable="yes" comments="Office file manipulation applications">Office</col>
      </row>
      <row>
